{
  "extends": ["next", "next/core-web-vitals", "eslint:recommended"],
  "globals": {
    "React": "readonly"
  },
  "rules": {
    "no-useless-escape": "off",
    "object-shorthand": "error",
    "no-unused-vars": 0,
    "no-return-await": "error",
    "react-hooks/exhaustive-deps": "off",
    "no-console": ["error", { "allow": ["warn", "error", "info", "trace"] }]
  }
}
